<?xml version="1.0"?>
<robot name="spine">

  <!-- Link: hind_spine (base link) -->
  <link name="hind_body">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.377"/>
      <inertia ixx="0.0001832" ixy="0" ixz="0" 
               iyy="0.0003327" iyz="0" 
               izz="0.0002337"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/hind_spine.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <!-- <collision>
